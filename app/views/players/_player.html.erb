<div id="<%= dom_id player %>" class="w-full sm:w-auto my-5 space-y-5">
  <div>
    <strong class="block font-medium mb-1">Game:</strong>
    <%= player.game_id %>
  </div>
  <div>
    <strong class="block font-medium mb-1">First name:</strong>
    <%= player.first_name %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Last name:</strong>
    <%= player.last_name %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Email:</strong>
    <%= player.email %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Cellphone:</strong>
    <%= player.cellphone %>
  </div>
</div>
